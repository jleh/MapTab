{
  "name": "FinMapTab",
  "description": "A tab extension for viewing your current location, date and time.",
  "short_name": "FinMapTab",
  "version": "1.0",
  "incognito": "split",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "geolocation",
    "cookies",
    "<all_urls>",
    "tabs"
  ],
  "icons": {
    "16": "assets/images/fin-icon16.png",
    "48": "assets/images/fin-icon48.png",
    "128": "assets/images/fin-icon128.png"
  },
  "manifest_version": 2
}
